local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("RGF3 V1")

local Bladeball = PhantomForcesWindow:NewSection("BladeBall Main RGF3 V1")

Bladeball:CreateToggle("Auto Parry RGF3", function()loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/MC%3ABlade%20Ball%20Parry",true))()
end)

